	year = {2021},
	note = {Number: D1},
	keywords = {notion},
	pages = {D605--D612},
	file = {Szklarczyk et al. - 2021 - The STRING database in 2021 customizable protein–.pdf:C\:\\Users\\franklin1\\Zotero\\storage\\C54FWVNZ\\Szklarczyk et al. - 2021 - The STRING database in 2021 customizable protein–.pdf:application/pdf;Szklarczyk et al. - 2021 - The STRING database in 2021 customizable protein–.pdf:C\:\\Users\\franklin1\\Zotero\\storage\\M4ZGZGDQ\\Szklarczyk et al. - 2021 - The STRING database in 2021 customizable protein–.pdf:application/pdf},
