	keywords = {Female, Humans, Estrogens, Algorithms, Computational Biology, Gene Expression Profiling, Cluster Analysis, Gene Expression Regulation, Neoplastic, Breast Neoplasms, Colonic Neoplasms, Gene Regulatory Networks, Internet, Reproducibility of Results, Software, notion},
	pages = {e13984},
	file = {Texte intégral:C\:\\Users\\franklin1\\Zotero\\storage\\9EDPJRGD\\Merico et al. - 2010 - Enrichment map a network-based method for gene-se.pdf:application/pdf},
