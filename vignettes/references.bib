	author = {Delignette-Muller, Marie Laure and Siberchicot, Aurélie and Larras, Floriane and Billoir, Elise},
	month = sep,
	year = {2023},
	note = {Pages: 2023.02.09.527852
Section: New Results},
	keywords = {notion},
	file = {Full Text PDF:C\:\\Users\\franklin1\\Zotero\\storage\\SMWH32BC\\Delignette-Muller et al. - 2023 - DRomics, a workflow to exploit dose-response omics.pdf:application/pdf},
