	year = {2007},
	note = {Number: 13},
	keywords = {notion},
	pages = {i29--i40},
	file = {Full Text PDF:C\:\\Users\\franklin1\\Zotero\\storage\\MR95FM6R\\Asur et al. - 2007 - An ensemble framework for clustering protein–prote.pdf:application/pdf;Full Text PDF:C\:\\Users\\franklin1\\Zotero\\storage\\QTCEQK7N\\Asur et al. - 2007 - An ensemble framework for clustering protein–prote.pdf:application/pdf;Full Text PDF:C\:\\Users\\franklin1\\Zotero\\storage\\VPLR56U9\\Asur et al. - 2007 - An ensemble framework for clustering protein–prote.pdf:application/pdf},
