	month = nov,
	year = {2012},
	note = {Number: D1},
	keywords = {notion},
	pages = {D808--D815},
	file = {Franceschini et al. - 2012 - STRING v9.1 protein-protein interaction networks,.pdf:C\:\\Users\\franklin1\\Zotero\\storage\\7VHBLYXX\\Franceschini et al. - 2012 - STRING v9.1 protein-protein interaction networks,.pdf:application/pdf;Franceschini et al. - 2012 - STRING v9.1 protein-protein interaction networks,.pdf:C\:\\Users\\franklin1\\Zotero\\storage\\99DZ72P4\\Franceschini et al. - 2012 - STRING v9.1 protein-protein interaction networks,.pdf:application/pdf},
