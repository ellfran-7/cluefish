	journal = {Food and Chemical Toxicology},
	author = {Antonijevic, Evica and Musilek, Kamil and Kuca, Kamil and Djukic-Cosic, Danijela and Curcic, Marijana and Miladinovic, Dejana Cupic and Bulat, Zorica and Antonijevic, Biljana},
	month = nov,
