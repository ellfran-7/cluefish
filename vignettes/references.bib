@incollection{hayes_chapter_2020,
	title = {Chapter 2 - {Dose} and dose-response relationships in toxicology},
	isbn = {978-0-12-815921-7},
	url = {https://www.sciencedirect.com/science/article/pii/B9780128159217000028},
	abstract = {Dose refers to the quantity of a chemical introduced into a biological organism in a given period of time. It can be expressed in many different forms. When a certain dose of therapeutic drug is administered to patients or experimental animals, this external dose does not necessarily reflect the internal exposure or the amount of chemical to which target cells or subcellular organelles are exposed. Statistical analyses are applied to all toxicity data, and individuals representing a population or cohort can show significantly different responses. For almost any medicine, there exists a fraction of the population that is highly susceptible to its toxicity (hypersensitivity) or resistant to toxicity (hypersensitivity). The cumulative dose-response can be used to predict whether a given compound will be harmful or safe at a specific dose and route of administration. Dose-response curves can show a wide variety of complex shapes and slopes that reflect the underlying mechanisms of toxicity. Different reference doses can be determined using a variety of approaches that have different levels of complexity, sensitivity, and significance. While a no-observed-adverse-effect level (NOAEL) can provide a rapid and rough value for assessing risk, more robust and refined methods are more suitable for setting safe limits in risk assessment for regulatory purposes.},
	language = {en},
	urldate = {2023-05-14},
	booktitle = {Loomis's {Essentials} of {Toxicology} ({Fifth} {Edition})},
	publisher = {Academic Press},
