	note = {Number: 23},
	keywords = {Animals, Humans, Mice, Mitochondria, Cell Line, Models, Biological, RNA, Messenger, Amino Acid Sequence, Cell Line, Tumor, Cyclic Nucleotide Phosphodiesterases, Type 5, DNA, Complementary, Expressed Sequence Tags, Molecular Sequence Data, Reverse Transcriptase Polymerase Chain Reaction, Sequence Homology, Amino Acid, Sphingomyelins, notion},
	pages = {17993--18002},
	file = {Texte intégral:C\:\\Users\\franklin1\\Zotero\\storage\\JAYUB4QS\\Wu et al. - 2010 - Identification and characterization of murine mito.pdf:application/pdf;Texte intégral:C\:\\Users\\franklin1\\Zotero\\storage\\GBM9TAMX\\Wu et al. - 2010 - Identification and characterization of murine mito.pdf:application/pdf},
