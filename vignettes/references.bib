	file = {Full Text PDF:C\:\\Users\\franklin1\\Zotero\\storage\\RKTAIKSR\\Andersen et Krewski - 2009 - Toxicity Testing in the 21st Century Bringing the.pdf:application/pdf;Full Text PDF:C\:\\Users\\franklin1\\Zotero\\storage\\Y7JRR825\\Andersen et Krewski - 2009 - Toxicity Testing in the 21st Century Bringing the.pdf:application/pdf;Snapshot:C\:\\Users\\franklin1\\Zotero\\storage\\EAABPR2T\\1683303.html:text/html},
